{
  "name": "dslink-java-dnp3",
  "version": "0.0.5",
  "description": "A dslink for the dnp3 protocol",
  "license": "Apache 2",
  "main": "bin/dslink-java-dnp3",
  "author": {
    "name": "Daniel Shapiro",
    "email": "danielsspam1272@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-dnp3"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-dnp3/issues"
  },
  "configs": {
  	"name": {
      "type": "string",
      "default": "DNP"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "dnp3.Main"
    }
  }
}
